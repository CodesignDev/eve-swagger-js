{
  "name": "eve-swagger",
  "version": "0.3.3",
  "description": "Promise-based, fluent API for EVE Online's ESI web interface",
  "license": "BSD-3-Clause",
  "author": "Michael Ludwig <lhkbob@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/lhkbob/eve-swagger-js.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepack": "npm run gen-docs; npm run build; npm test",
    "build": "babel src -d lib --ignore **/*.spec.js,*/__tests__/*,*/__mocks__/*",
    "test": "jest",
    "gen-docs": "jsdoc --configure ./jsdoc.json"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "lib/"
    ],
    "cacheDirectory": "node_modules/.jest-cache"
  },
  "publishConfig": {
    "tag": "dev"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.6",
    "bottleneck": "^1.15.1",
    "callable-instance": "^1.0.0",
    "moment": "^2.17.1",
    "node-cache": "^4.1.0",
    "superagent": "^1.7.1",
    "verror": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "ink-docstrap": "^1.3.0",
    "jest": "^19.0.2",
    "jsdoc": "^3.4.3",
    "sync-request": "^4.0.2"
  }
}
